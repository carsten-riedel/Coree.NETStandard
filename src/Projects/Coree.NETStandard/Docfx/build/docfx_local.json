{
  "metadata": [
    {
      "noRestore": true,
      "src": [
        {
          "src": "../../../Coree.NETStandard",
          "files": [
            "**/*.csproj"
          ]
        }
      ],
      "dest": "api"
    }
  ],
  "build": {
    "content": [
      {
        "files": [
          "**/*.{md,yml}"
        ]
      }
    ],
    "resource": [
      {
        "files": [
          "images/**"
        ]
      }
    ],
    "output": "../result/local",
    "template": [
      "default",
      "modern"
    ],
    "globalMetadata": {
      "_appName": "Coree.NETStandard",
      "_appTitle": "Coree.NETStandard",
      "_appFooter": "<pre></pre>"
    }
  }
}